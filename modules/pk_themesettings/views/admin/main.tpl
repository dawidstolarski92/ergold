<script>var tsDir = "{$path}"</script>
<form action="{$action}" enctype="multipart/form-data" class="list_options" id="themesettings ts-prefix" method="post">
	{$message nofilter}
	<fieldset>

		<div class="heading">
			<div class="module-title">
				<img src="{$path}logo.png" width="16" height="16" alt="" title="" />
				{l s='Theme Settings' d='Modules.ThemeSettings.Shop'}
			</div>
			<div class="buttons_section">
			
				<input type="submit" name="submitThemeSettings" value="{l s='Apply Settings' d='Modules.ThemeSettings.Shop'}" class="button" />
			</div>
		</div>

		<div class="tabscontainer">

			<div class="tabs">						
				{foreach from=$sections item=tab name=tab}
					{assign var="tid" value=$smarty.foreach.tab.index+1}
				    <div class="tab{if ($s.tab_number == $tid)} selected{/if}" id="tab_menu_{$tid}">
				    	<div class="link">{if isset($tab.icon)}<svg class="svgic"><use xlink:href="#si-{$tab.icon}"></use></svg>{/if}{$tab.title}</div>
				    	<div class="arrow"></div>
				    	<input type="radio" class="hidden" name="tab_number" id="tab_{$tid}" value="{$tid}" {if $s.tab_number == $tid}checked='checked'{/if} />
				    </div>
				{/foreach}
	        </div>		

	        <div class="curvedContainer bootstrap">	  
			{foreach from=$sections item=section name=section}
				{assign var="tid" value=$smarty.foreach.section.index+1}
				<div class="tabcontent" id="tab_content_{$tid}" {if $s.tab_number == $tid}style='display:block'{/if}>
					<div class="va-content">
					{if isset($section.content.hooks)}
						{include file="{$tpl_path}controllers/module.tpl" section=$section.content.hooks}
					{/if}
					{foreach from=$section.options_list item=option}
						{include file="{$tpl_path}controllers/{$option.type}.tpl" section=$option}
					{/foreach}
					</div>
				</div>
			{/foreach}
			</div>

		</div>
		
		<div class="footer_section">
			<div class="footer-buttons">
			<input type="submit" name="resetThemeSettings" value="{l s='Reset Settings' d='Modules.ThemeSettings.Shop'}" class="button" id="reset_sett" />
			</div>
			<span>{$version} | Powered by <a target="_blank" href="http://promokit.eu">Promokit Co.</a></span>
		</div>

	</fieldset>

</form>